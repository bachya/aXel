<?xml version="1.0" encoding="UTF-8"?><highlightDefinition version="1.0">	<name>XML</name>	<contexts defaultColor="#0" caseSensitive="no">				<highlightContext name="Comment" highlightColor="#236E25">			<startRegEx>&lt;!--</startRegEx>			<endRegEx>--&gt;</endRegEx>		</highlightContext>

		<highlightContext name="CDATA" highlightColor="#FF0000">			<startRegEx>&lt;!\s*\[CDATA\s*\[</startRegEx>			<endRegEx>\]\s*\]\s*&gt;</endRegEx>		</highlightContext>
		
		<highlightContext name="Doctype" highlightColor="#236E25">			<startRegEx>&lt;!</startRegEx>			<endRegEx>&gt;</endRegEx>		</highlightContext>
		
		<highlightContext name="Escape" highlightColor="#236E25">			<startRegEx>&lt;\?</startRegEx>			<endRegEx>\?&gt;</endRegEx>		</highlightContext>
					        <highlightContext name="Tags" highlightColor="#881280">            <entryRegEx>(&lt;[^&gt;]*&gt;)</entryRegEx>
            
            <highlightContext name="Strings in Tags" highlightColor="#1A1AA6">                <entryRegEx>("[^"&gt;&lt;]*")</entryRegEx>            </highlightContext>
            
            <highlightContext name="Single Strings in Tags" highlightColor="#1A1AA6">                <entryRegEx>('[^'&gt;&lt;]*')</entryRegEx>            </highlightContext>
            
            <highlightContext name="Attributes in Tags" highlightColor="#994500">                <entryRegEx>([\w-]*)[ \t]*=(?=[ \t]*"[^"&gt;&lt;]*")</entryRegEx>            </highlightContext>            </highlightContext>
                <highlightContext name="Entities" highlightColor="#881280">            <entryRegEx>(&amp;\w{1,8};)</entryRegEx>        </highlightContext>	</contexts></highlightDefinition>